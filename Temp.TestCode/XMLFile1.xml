<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
	<xsl:template match="*">
		<Action>
			<xsl:if test="count(//DecisionsNotProcessedItemItem) &gt; 0">
				<DecisionProcess>
					<xsl:for-each select="//DecisionsNotProcessedItemItem">
						<xsl:copy-of select="DecisionID"/>
					</xsl:for-each>
				</DecisionProcess>
			</xsl:if>
		</Action>
	</xsl:template>
</xsl:stylesheet>