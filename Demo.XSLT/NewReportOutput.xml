<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:qbo="http://services.quandis.com/" version="1.0">
	<xsl:output method="xml" omit-xml-declaration="no" indent="no" />
	<xsl:template match="*">
		<Action>
			<AttachmentID>
				<xsl:value-of select="//AttachmentID"/>
			</AttachmentID>
			<TriggerWorkflow>true</TriggerWorkflow>
			<LRREventID>812</LRREventID>
			<StoredProcedure>pPublicDataImportFromPOR</StoredProcedure>
			<isPORDataCopy>No</isPORDataCopy>
		</Action>
	</xsl:template>
</xsl:stylesheet>